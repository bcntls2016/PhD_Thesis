@article{Rader2017,
  title = {Multicomponent correlated-basis-function method and its application to multilayered dipolar Bose gases},
  author = {Michael Rader and Martin Hebenstreit and Robert E. Zillich},
  journal = {Phys. Rev. A}, volume=95, pages={033625}, numpages=13, year=2017,
  abstract = {We present a method for calculating the dynamics of a bosonic mixture, the multi-component correlated basis function (CBF) method. For single components, CBF results for the excitation energies agree quite well with experimental results, even for highly correlated systems like {$^4$He}, and recent systematic improvements of CBF achieve perfect agreement. We give a full derivation of multi-component CBF, and apply the method to a dipolar Bose gas cut into two-dimensional layers by a deep optical lattice, with coupling between layers due to the long-ranged dipole-dipole interaction. We consider the case of strong coupling, leading to large positive interlayer correlations. We calculate the spectrum for a system of 8 layers and show that the strong coupling can lead to a simpler spectrum than in the uncoupled case, with a single peak carrying most of the spectral weight. },
  doi = {10.1103/PhysRevA.95.033625},
  url = {https://link.aps.org/doi/10.1103/PhysRevA.95.033625},
  url2 = {https://arxiv.org/abs/1611.04757}}
  
