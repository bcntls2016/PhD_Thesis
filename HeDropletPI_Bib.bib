@Article{SmolarekPCCP:2010,
author ="Smolarek, Szymon and Rijs, Anouk M. and Buma, Wybren Jan and Drabbels, Marcel",
title  ="Absorption spectroscopy of adenine{,} 9-methyladenine{,} and 2-aminopurine in helium nanodroplets",
journal  ="Phys. Chem. Chem. Phys.",
year  ="2010",
volume  ="12",
issue  ="48",
pages  ="15600-15606",
publisher  ="The Royal Society of Chemistry",
doi  ="10.1039/C0CP00746C",
url  ="http://dx.doi.org/10.1039/C0CP00746C",
abstract  ="High-resolution absorption spectra of adenine{,} 9-methyladenine and 2-aminopurine in helium nanodroplets have been recorded. In contrast to molecular beam experiments{,} large variations in linewidths are observed for adenine and 9-methyladenine. At the same time{,} the spectrum of 2-aminopurine remains sharp upon solvation in helium droplets. The line broadening observed for adenine and 9-methyladenine is attributed to a significant decrease of the lifetime of the 1Lb([small pi][small pi]*) state and of 1n[small pi]* levels vibronically coupled to this state. The origin of the lifetime reduction is argued to be related to the increased accessibility of the 1n[small pi]*/1Lb([small pi][small pi]*) conical intersection upon solvation of these molecules in liquid helium."}

